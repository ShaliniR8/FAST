<div class="col-xs-12 col-sm-12 mt20">
  <div class="table-responsive">
    <table class="table table-striped table-bordered" width="100%">
      <thead>
        <tr>
          <th>ID</th>
          <th>Value</th>
          <th>Count</th>
        </tr>
      </thead>
      <tbody>
        <%@data.each_with_index do |(k, v), index|%>
          <tr>
            <td><%=index+1%></td>
            <td><%=k%></td>
            <td><%=v%></td>
          </tr>
        <%end%>
      </tbody>
    </table>
  </div>
</div>




