<div class="panel-body">
  <div class="row">
    <div class='col-xs-12'>
      <table class="table table-hover" id="airports<%=@field_id%>">
        <thead>
          <tr>
          <%@headers.keys.each do |k|%>
            <th><%=k%></th>
          <%end%>
          </tr>
        </thead>
        <tbody>
          <%@records.each do |u|%>
            <tr uid="<%=u.id%>" class="list_tr">
            <%@headers.values.each do |v|%>
              <td><%=u.send(v)%></td>
            <%end%>
            </tr>
          <%end%>
        </tbody>
      </table>
    </div>
  </div>
</div>



