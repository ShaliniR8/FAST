<?xml version="1.0" encoding="utf-8"?>
<Airline<%=BaseConfig.airline[:code]%>_CSAP>
  <evnet_id><%if @submission.record.present? && @submission.record.report.present?%><%=@submission.record.report.id%><%end%></event_id>
  <reporter_info>
    <name><%=@submission.created_by.full_name%></name>
    <address><%=@submission.created_by.address%></address>
    <city><%=@submission.created_by.city%></city>
    <state><%=@submission.created_by.state%></state>
    <zip><%=@submission.created_by.zipcode%></zip>
    <work_phone><%=@submission.created_by.work_phone_number%></work_phone>
    <mobile_phone><%=@submission.created_by.mobile_number%><mobile_phone/>
  </reporter_info>
  <equipment_info>
    <aircraft_type><%=@submission.find_field('Aircraft Type')%></aircraft_type>
  </equipment_info>
  <flight_info>
    <flight_number><%=@submission.find_field('Flight Number')%></flight_number>
    <departure><%=@submission.find_field('Departure Airport')%></departure>
    <destination><%=@submission.find_field("Scheduled Arrival Airport")%></destination>
  </flight_info>
  <event_info>
    <date><%=@submission.get_date%></date>  <!-- Date of incident -->
    <time><%=@submission.get_time%></time>  <!-- local time preferred, or 14:00Z for UTC -->
    <flight_phase><%=@submission.find_field("Flight Phase at Start of Event")%></flight_phase>
    <%@submission.template.categories.each do |category|%>
      <%if category.title=="Narratives"%>
        <%next%>
      <%end%>
      <%category.fields.each do |field|%>
        <%sub_field=@submission.submission_fields.find_by_fields_id(field.id)%>
        <%if sub_field.present?%> 
          <%if sub_field.export.present?%>
            <%=sub_field.export%>
          <%end%>
        <%end%>
      <%end%>
    <%end%>
    <star_sid></star_sid>
    <redeye></redeye>  <!-- Example add't "Yes" or "No" field -->
    <vmc_imc></vmc_imc>
    <significant_weather></significant_weather>
    <description><%=@submission.get_narratives%></description>  <!-- our maximum is 16000 characters for both description and recommendations -->
    <recommendations></recommendations>  <!-- or some other narrative field. -->
  </event_info>
</Airline<%=BaseConfig.airline[:code]%>_CSAP>
