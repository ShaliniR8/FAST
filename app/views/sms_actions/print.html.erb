<div class="title">
  Corrective Action #
  <%=@corrective_action.get_id%>: <%=@corrective_action.title%>
</div>

<%=render "/forms/print_fields",
  :fields => SmsAction.get_meta_fields('show'),
  :owner => @corrective_action,
  :deidentified => @deidentified
%>

<!-- Print Risk Matrix -->
<% if CONFIG::GENERAL[:base_risk_matrix]%>
  <%=render "/shared/print_risk_matrix",
    :owner => @corrective_action
  %>
<%else%>
  <%=render "/shared/#{AIRLINE_CODE}/print_risk_matrix",
    :owner => @corrective_action
  %>
<%end%>

<%=render '/sras/print_cost', :owner => @corrective_action, :deidentified => @deidentified
%>
