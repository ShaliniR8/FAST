<% headers = ['Title','Department','Reference Number', 'Reference','Requirement','Status','Level of Compliance','Comment'] %>
<%= CSV.generate_line headers %>
<%@audit.items.each do |i|%>
  <%= CSV.generate_line([
    i.title,
    i.department,
    i.reference_number,
    i.reference,
    i.requirement,
    i.status,
    i.level_of_compliance,
    i.comment]).html_safe%>
<%end%>
