<%
  items = @record.checklist_header_items
  titles = items.map(&:title)
  examples = items.map do |item|
    if item.data_type.match /radio|dropdown/
      item.options
    else
      'Example Text' unless item.editable
    end
  end
%>
<%=
  CSV.generate_line titles.push('Header? (Y/N)')
%><%=
  CSV.generate_line examples.push('Y')
%>